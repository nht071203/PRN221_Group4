            $('.discussion-reply-form-append .resrtimg').html(html);
            if (imagerating.length > 0) {
                $('.discussion-reply-form-append .resrtimg').removeclass('hide');
            }
            else {
                $('.discussion-reply-form-append .resrtimg').addclass('hide');
